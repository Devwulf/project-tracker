    "setup": "npm install tailwindcss postcss-cli autoprefixer -D"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.3",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.4.6"
